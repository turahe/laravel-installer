{
    "name": "turahe/laravel-installer",
    "description": "Laravel web installer",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "trait",
        "laravel",
        "eloquent",
        "installer",
        "Web"
    ],
    "authors": [
        {
            "name": "Nur Wachid",
            "email": "wachid@outlook.com",
            "homepage": "https://turahe.id",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/database": "5.* |^6.0 | ^7.0 | ^8.0",
        "illuminate/support": "5.* |^6.0 | ^7.0 | ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "3.*| ^4.0| ^5.0 | ^6.0",
    },
    "autoload": {
        "psr-4": {
            "Turahe\\LaravelInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Turahe\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable" : true
}
